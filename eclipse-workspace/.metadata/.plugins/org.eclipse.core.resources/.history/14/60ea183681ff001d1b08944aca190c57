package workout;

import java.util.Scanner;

public class palindrome {
	public static void min(String[] args) {
		Scanner input = new Scanner(System.in);
		int n = input.nextInt();
		int temp = n;
		int a;
		int s;
		while (n > 0) {
			a = n % 10;
			s = a;
			n = n / 10;
		}
		if (temp == s) {
			System.out.println("its palindrome");
		} else {
			System.out.println("not palindrome");
		}
	}

}
